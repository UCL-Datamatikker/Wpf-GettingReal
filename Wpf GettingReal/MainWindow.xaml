<Window x:Class="Wpf_GettingReal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_GettingReal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Register" Content="Register Comapny" HorizontalAlignment="Left" Margin="300,50,0,0" VerticalAlignment="Top" Click="Register_Click"/>
        <Button x:Name="Login" Content="Already Registered" HorizontalAlignment="Left" Margin="300,100,0,0" VerticalAlignment="Top" Click="Login_Click"/>
    </Grid>
</Window>
