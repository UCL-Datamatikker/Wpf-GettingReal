<Window x:Class="Wpf_GettingReal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_GettingReal"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Top" HorizontalAlignment="Left" Orientation="Horizontal">
            <Button Name="btnCompany" Content="Company" Margin="5,5,5,5" Click="btnCompany_Click"/>
            <Button Name="btnIncome" Content="Income" Margin="5,5,5,5" Click="btnIncome_Click"/>
            <Button Name="btnExpense" Content="Expense" Margin="5,5,5,5" Click="btnExpense_Click"/>
        </StackPanel>
        <Frame Name="Main" Margin="0,30,0,27" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
