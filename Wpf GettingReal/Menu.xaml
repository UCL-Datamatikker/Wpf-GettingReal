<Window x:Class="Wpf_GettingReal.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf_GettingReal"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800">
    <Grid>
        <Button x:Name="Transactions" Content="Transaktioner" HorizontalAlignment="Left" Margin="300,50,0,0" VerticalAlignment="Top" Click="Transactions_Click"/>
        <Button x:Name="Balance" Content="Se regnskab" HorizontalAlignment="Left" Margin="300,100,0,0" VerticalAlignment="Top" Click="Balance_Click"/>
        <Button x:Name="Commit" Content="Registrér årsregnskab" HorizontalAlignment="Left" Margin="300,150,0,0" VerticalAlignment="Top" Click="Commit_Click"/>

    </Grid>
</Window>
